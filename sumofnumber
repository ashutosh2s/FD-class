
import java.sql.SQLOutput;
import java.util.Scanner;

public class Sumofnumber {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sumofdigits = 0;

        for(; n >0;){
            sumofdigits = sumofdigits+ n%10;
            n = n/10;
        }
        System.out.println(sumofdigits);
    }

}
